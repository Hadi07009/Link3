                      tbl_fpi_consignment_info.consign_no = tbl_fpi_custom_clearing.consign_number ON 
                      tbl_fpi_cargo_landing_hdr.consign_number = tbl_fpi_consignment_info.consign_no AND 
                      tbl_fpi_cargo_landing_hdr.ref_no = tbl_fpi_consignment_info.ref_no RIGHT OUTER JOIN
                      tbl_fpi_approval_data ON tbl_fpi_consignment_info.ref_no = tbl_fpi_approval_data.ref_no RIGHT OUTER JOIN
                      PuTr_IN_Det INNER JOIN
                      InMa_Itm_Det ON PuTr_IN_Det.IN_Det_Icode = InMa_Itm_Det.Itm_Det_Icode AND InMa_Itm_Det.Itm_Det_Type_flag = 'R' INNER JOIN
                      PuTr_IN_Hdr ON PuTr_IN_Det.IN_Det_Ref = PuTr_IN_Hdr.IN_Hdr_Ref AND PuTr_IN_Hdr.IN_Hdr_Status &lt;&gt; 'REJ' ON 
                      tbl_fpi_approval_data.mpr_ref_no = PuTr_IN_Det.IN_Det_Ref AND tbl_fpi_approval_data.mpr_itm_code = PuTr_IN_Det.IN_Det_Icode LEFT OUTER JOIN
                      tbl_Comments AS a ON a.ref_no = PuTr_IN_Det.IN_Det_Ref AND a.role_as = 'mpr1' LEFT OUTER JOIN
                      tbl_Comments AS b ON b.ref_no = PuTr_IN_Det.IN_Det_Ref AND b.role_as = 'mpr2' LEFT OUTER JOIN
                      tbl_Comments AS c ON c.ref_no = PuTr_IN_Det.IN_Det_Ref AND c.role_as = 'mpr3' LEFT OUTER JOIN
                      tbl_Comments AS d ON d.ref_no = tbl_fpi_approval_data.ref_no AND d.role_as = 'PIENT' LEFT OUTER JOIN
                      tbl_Comments AS e ON e.ref_no = tbl_fpi_approval_data.ref_no AND e.cur_status = 'APP'
