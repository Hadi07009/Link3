                      PuTr_IN_Hdr.IN_Hdr_St_DATE
FROM         tbl_fpi_cargo_landing_det RIGHT OUTER JOIN
                      tbl_fpi_cargo_landing_hdr ON tbl_fpi_cargo_landing_det.ref_no = tbl_fpi_cargo_landing_hdr.ref_no AND 
